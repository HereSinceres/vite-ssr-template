{
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node ./server",
    "// Build for production": "",
    "build:clean": "rimraf dist/",
    "build:vite": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "// node serve for production": "",
    "server:build": "npm run build:clean && npm run build:vite",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "// Static serve for production": "",
    "server:build:prerender:prod": "npm run build:clean && npm run build:vite && npm run build:prerender",
    "server:prod:prerender": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@ProjectName/lib-common": "file:../lib-common",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.13",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.3",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "ua-parser-js": "^1.0.2",
    "vite": "^2.7.2",
    "vite-plugin-mdx": "3.5.10",
    "vite-plugin-ssr": "0.3.37"
  },
  "// Needed for Yarn workspaces": "",
  "name": "example-file-structure-domain-driven",
  "version": "0.0.0"
}
